      // "infoPlist": {
      //    "UIBackgroundModes": [
      //      "fetch",
      //      "remote-notification"
      //    ]
      // }

    },
    "android": {
      "package": "com.firstcontact.firstcontact1",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },

    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "082fd262-a56e-4101-be44-f8d1713050fb"
      }
    }
  },
  "plugins": [
